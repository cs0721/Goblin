@model Assignment_Di.Models.AspNetUsers

@{
    ViewBag.Title = "Delete";
}

<link href="~/Content/s1.css" rel="stylesheet" type="text/css" />
<link href="~/Content/createTable1.css" rel="stylesheet" type="text/css" />

<br />
<h2 style="font-weight:800">Delete User</h2> <br />
<h3>Are you sure you want to delete this?</h3>
<br />
<table class="delete">
    <tr>
        <th width="200">User Name</th>
        <td width="30"></td>
        <td width="400">
            <dd>
                @Html.DisplayFor(model => model.UserName)
            </dd>
        </td>
    </tr>
    <tr class="height"></tr>
    <tr>
        <th>User Email</th>
        <td></td>
        <td>
            <dd>
                @Html.DisplayFor(model => model.Email)
            </dd>
        </td>
    </tr>
</table>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <p>
        <br /> <br />
        <input class="OneButton" type="submit" value="Delete" />
        <button class="OneButton" onclick="location.href='@Url.Action("Index", "AspNetUsers")';
            return false;">
            Back to List
        </button>
    </p>
}
